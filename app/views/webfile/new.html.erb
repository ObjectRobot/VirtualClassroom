<h1>Upload Webfile</h1>
<form action="/webfile" method="post" enctype="multipart/form-data">
	<div class="form-group">
		<label>Title:</label>
		<input type="text" name="webfile[title]" class="form-control">
	</div>
	<div class="form-group">
		<label>Image:</label>
		<input type="file" name="webfile[image]" class="form-control">
	</div>
	<input type="submit" value= "Upload" class="btn btn-primary">
</form>
<% if flash[:errors] %>
	<% flash[:errors].each do |e| %>
	<div class="alert alert-danger"><%= e %></div>
	<% end %>
<% end %>