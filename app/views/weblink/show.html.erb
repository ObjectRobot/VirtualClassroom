<h1>View <%= @weblink.title %></h1>
<form action="/weblink/ <%= @weblink.id %>" method="post">
	<input type="hidden" name="_method" value="delete">
	<div>
		<label>Title:</label>
		<input type="text" name="weblink[title]" value="<%= @weblink.title%>" disabled>
	<div>
		<label>Url:</label>
		<span><a href = "<%= @weblink.url%>" target="_blank"><%= @weblink.url%></a><span>
	</div>
</form>